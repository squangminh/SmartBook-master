@inject AdwardSoft.Provider.Common.IUserSession userSession
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"]</title>



    <environment>
        <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
        <link href="~/admin/global_assets/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
        <link href="~/admin/global_assets/css/icons/fontawesome/styles.min.css" rel="stylesheet" />
        <link href="~/admin/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
        <link href="~/admin/assets/css/bootstrap_limitless.min.css" rel="stylesheet" type="text/css">
        <link href="~/admin/assets/css/layout.min.css" rel="stylesheet" type="text/css">
        <link href="~/admin/assets/css/components.min.css" rel="stylesheet" type="text/css">
        <link href="~/admin/assets/css/colors.min.css" rel="stylesheet" type="text/css">
        <link href="~/admin/global_assets/css/Custom.css" rel="stylesheet" />
        <link href="~/lib/datatable/fixedcolumn/fixedColumns.min.css" rel="stylesheet" />
        <link href="~/lib/datatable/rowgroup/rowGroup.min.css" rel="stylesheet" />
        <link href="~/lib/datatable/buttons/buttons.min.css" rel="stylesheet" />
        <link href="~/lib/datetimepicker/datetimepicker.css" rel="stylesheet" />
        <link href="~/admin/assets/css/Search.css" rel="stylesheet" />
        <!-- /global stylesheets -->
        @RenderSection("StyleSheet", required: false)
    </environment>

</head>
<body class="style-scroll">
    <!-- Main navbar -->
    <div class="navbar navbar-expand-md navbar-dark">
        <div class="navbar-brand">
            <a class="d-inline-block">ADWARDSOFT</a>
            @*<a href="index.html" class="d-inline-block">
                    <img src="~/admin/global_assets/images/logo_light.png" alt="">
                </a>*@
        </div>
        <div class="d-md-none">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-mobile">
                <i class="icon-tree5"></i>
            </button>
            <button class="navbar-toggler sidebar-mobile-main-toggle" type="button">
                <i class="icon-paragraph-justify3"></i>
            </button>
        </div>
        <div class="collapse navbar-collapse" id="navbar-mobile">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a href="#" class="navbar-nav-link sidebar-control sidebar-main-toggle d-none d-md-block">
                        <i class="icon-paragraph-justify3"></i>
                    </a>
                </li>
            </ul>
            <span class="navbar-text ml-md-3 mr-md-auto">
                @*<span class="badge bg-success">Online</span>*@
            </span>
            <ul class="navbar-nav">
                <li class="nav-item dropdown dropdown-user">
                    <a href="#" class="navbar-nav-link dropdown-toggle" data-toggle="dropdown">
                        <img src="~/upload/Images/Avatar/@userSession.Avatar" class="rounded-circle" alt="">
                        <span>@userSession.FullName</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <form asp-controller="User" asp-action="Logout" method="post" id="fLogout" class="navbar-right">
                            <a href="@Url.Action("Profile","Home")" class="dropdown-item"><i class="icon-user-plus"></i> Thông tin</a>
                            <a href="@Url.Action("_ChangePasswordForm","Home")" class="dropdown-item modal-link"><i class="icon-lock"></i> Đổi mật khẩu</a>

                            @*<a href="#" class="dropdown-item"><i class="icon-comment-discussion"></i> Messages <span class="badge badge-pill bg-blue ml-auto">58</span></a>
                                <div class="dropdown-divider"></div>*@
                            @*<a href="#" class="dropdown-item"><i class="icon-cog5"></i> Account settings</a>*@
                            <button type="submit" class="dropdown-item"><i class="icon-switch2"></i> Đăng xuất</button>
                        </form>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page content -->
    <div class="page-content">
        <!-- Main sidebar -->
        <div class="sidebar sidebar-dark sidebar-main sidebar-expand-md">
            <!-- Sidebar mobile toggler -->
            <div class="sidebar-mobile-toggler text-center">
                <a href="#" class="sidebar-mobile-main-toggle">
                    <i class="icon-arrow-left8"></i>
                </a>
                Navigation
                <a href="#" class="sidebar-mobile-expand">
                    <i class="icon-screen-full"></i>
                    <i class="icon-screen-normal"></i>
                </a>
            </div>
            <!-- /sidebar mobile toggler -->
            <!-- Sidebar content -->
            <div class="sidebar-content">
                <!-- User menu -->
                <div class="sidebar-user">
                    <div class="card-body">
                        <div class="media">
                            <div class="mr-3">
                                <a href="#"><img src="~/upload/Images/Avatar/@userSession.Avatar" width="38" height="38" class="rounded-circle" alt=""></a>
                            </div>
                            <div class="media-body">
                                <div class="media-title font-weight-semibold"> @userSession.FullName</div>
                                <div class="font-size-xs opacity-50">
                                    <i class="icon-mail5 font-size-sm"></i> &nbsp;@userSession.UserName
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /user menu -->
                <!-- Main navigation -->
                <div class="card card-sidebar-mobile">
                    <ads-menuleft id="MenuLeft"></ads-menuleft>
                    <!-- /main -->
                </div>
                <!-- /main navigation -->
            </div>
            <!-- /sidebar content -->

        </div>
        <!-- /main sidebar -->
        <!-- Main content -->
        <div class="content-wrapper">
            <!-- Page header -->
            <div class="page-header page-header-light">
            </div>
            <!-- /page header -->
            <!-- Content area -->
            <div class="content">
                @RenderBody()
            </div>
            <!-- /content area -->
            <!-- Footer -->
            <div class="navbar navbar-expand-lg navbar-light">
                <div class="text-center d-lg-none w-100">
                    <button type="button" class="navbar-toggler dropdown-toggle" data-toggle="collapse" data-target="#navbar-footer">
                        <i class="icon-unfold mr-2"></i>
                        Footer
                    </button>
                </div>
                <div class="navbar-collapse collapse" id="navbar-footer">
                    <span class="navbar-text">
                        &copy; 2015 - 2019. <a href="#">Copyright</a> by <a href="http://www.adwardsoft.com" target="_blank">AdwardSoft Corp</a>
                    </span>
                    @*<ul class="navbar-nav ml-lg-auto">
                            <li class="nav-item"><a href="#" class="navbar-nav-link" target="_blank"><i class="icon-lifebuoy mr-2"></i> Support</a></li>
                            <li class="nav-item"><a href="#" class="navbar-nav-link" target="_blank"><i class="icon-file-text2 mr-2"></i> Docs</a></li>
                            <li class="nav-item"><a href="#" class="navbar-nav-link font-weight-semibold"><span class="text-pink-400"><i class="icon-versions mr-2"></i> Version</span></a></li>
                        </ul>*@
                </div>
            </div>
            <!-- /footer -->
        </div>
        <!-- /main content -->
    </div>

    <environment>
        <!-- Core JS files -->
        <script src="~/admin/global_assets/js/main/jquery.min.js"></script>
        <script src="~/admin/global_assets/js/main/bootstrap.bundle.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/loaders/blockui.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/visualization/echarts/echarts.min.js"></script>
        <!-- /core JS files -->
        <!-- Theme JS files -->
        <script src="~/admin/global_assets/js/plugins/visualization/d3/d3.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/visualization/d3/d3_tooltip.js"></script>

        <script src="~/admin/global_assets/js/plugins/forms/selects/bootstrap_multiselect.js"></script>
        <script src="~/lib/datetimepicker/moment.min.js"></script>
        <script src="~/lib/datetimepicker/datetimepicker.js"></script>
        <script src="~/admin/global_assets/js/plugins/pickers/daterangepicker.js"></script>

        <script src="~/admin/global_assets/js/plugins/forms/styling/uniform.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/forms/styling/switchery.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/forms/styling/switch.min.js"></script>
        <script src="~/admin/global_assets/js/demo_pages/form_checkboxes_radios.js"></script>



        <!-- Button with spinner -->
        <script src="~/admin/global_assets/js/plugins/buttons/spin.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/buttons/ladda.min.js"></script>
        <!-- TinyMCE -->

        <script src="~/lib/tinymce/tinymce_5.0.7.min.js"></script>
        <!-- Alert -->
        <script src="~/admin/global_assets/js/plugins/notifications/sweet_alert.min.js"></script>
        <!-- Datatable -->

        <script src="~/lib/datatable/jquery.dataTables.min.js"></script>
        <script src="~/lib/datatable/dataTables.min.js"></script>
        <script src="~/lib/datatable/rowgroup/rowGroup.min.js"></script>
        <script src="~/lib/datatable/fixedcolumn/fixedColumns.min.js"></script>
        <script src="~/lib/datatable/buttons/dataTables.buttons.min.js"></script>

        <script src="~/admin/global_assets/js/plugins/tables/datatables/datatables.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/tables/datatables/extensions/buttons.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/tables/datatables/extensions/pdfmake/pdfmake.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/tables/datatables/extensions/pdfmake/vfs_fonts.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/tables/datatables/extensions/jszip/jszip.min.js"></script>
        <!-- Noty -->
        <script src="~/admin/global_assets/js/plugins/notifications/jgrowl.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/notifications/noty.min.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/forms/selects/select2.min.js"></script>
        <script src="~/admin/global_assets/js/plugins/notifications/pnotify.min.js"></script>

        <script src="~/lib/app/modals.js"></script>
        <script src="~/lib/app/ADS.js"></script>
        <script src="~/admin/global_assets/js/main/Custom.js"></script>
        <script src="~/lib/jsrender/jsrender.min.js"></script>
        <script src="~/lib/jsrender/jsviews.min.js"></script>

        <script src="~/lib/app/ChartCustom.js"></script>

        <script src="~/admin/global_assets/js/main/jquery.isotope.js"></script>
        <script src="~/admin/assets/js/app.js"></script>
        <script src="~/admin/global_assets/js/plugins/pickers/color/spectrum.js"></script>
        <script src="~/admin/global_assets/js/plugins/tables/datatables/extensions/fixed_columns.min.js"></script>
        @RenderSection("Scripts", required: false)
    </environment>
</body>
</html>
