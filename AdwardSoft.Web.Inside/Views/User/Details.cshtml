<script type="text/javascript">
    function currentPass() {

        if ($("#OldPassword").val() == "") {
            $("#divOldPassword").show(500);
            return false;
        }
        $("#divOldPassword").hide(500);
        return true;
    }

    function newPass() {

        if ($("#NewPassword").val() == "" || $("#NewPassword").val().length < 6) {
            $("#divNewPassword").show(500);
            return false;
        }
        $("#divNewPassword").hide(500);
        return true;
    }

    function repeat() {

        if ($("#RepeatPassword").val() != "" && ($("#NewPassword").val() != $("#RepeatPassword").val())) {
            $("#divRepeatPassword").show(500);
            return false;
        }
        $("#divRepeatPassword").hide(500);
        return true;
    }

    function checkAll() {
        currentPass(); newPass(); repeat();
        if (currentPass() && newPass() && repeat())
            return true;
        else
            return false;
    }


    $("#fChangePass").submit(function(e) {

    e.preventDefault();

    if (!checkAll())
        window.scrollTo(0, 0);
    else {
        var form = $('#fChangePass');
        var formAction = $(this).attr('action');
        var fdata = new FormData(form.get(0));

        fdata.append('model[Username]', $("#Username").val());
        fdata.append('model[OldPassword]', $("#OldPassword").val());
        fdata.append('model[NewPassword]', $("#NewPassword").val());

        $.ajax({
            type: 'PUT',
            url: formAction,
            data: fdata,
            processData: false,
            contentType: false,
            async: true
        }).done(function (result) {
            debugger
            console.log(result);
            if (result.succeeded) {
                swal({
                    title: 'Lưu thành công!',
                    text: '',
                    type: 'success',
                    showCloseButton: true,
                    confirmButtonClass: 'btn btn-primary',
                    cancelButtonClass: 'btn btn-light'
                }).then(function() {
                    window.location = '@Url.Action("Index", "Users")';
                }).catch(swal.noop);
            } else {
                alert(result.errors[0].description);
            }
        }).fail(function() {
            console.log("Error!");
        });
    }
});

</script>